#!/bin/bash

NUM=104
while [ $NUM -lt 114 ]; do
    cp ./append_congress.sh ../pass$NUM/
    cp ./append_congress.sh ../fail$NUM/
    ../pass$NUM/append_congress.sh $NUM
    ../fail$NUM/append_congress.sh $NUM
    
    rm ../pass$NUM/$NUM_append_congress.sh
    rm ../fail$NUM/$NUM_append_congress.sh
    let NUM=$NUM+1
done
